{
  "name": "netlify-gocommerce-widget",
  "version": "0.0.1",
  "releaseVersion": "v1",
  "description": "Netlify GoCommerce widget for easy integration",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config webpack.config.babel.js",
    "start": "serve build -s -c 1",
    "react-demo": "cd example && yarn && yarn start",
    "prestart": "run-s build",
    "version": "run-s release",
    "prepublish": "run-s build",
    "build": "run-p build:*",
    "build:bundle": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "build:umd": "webpack --config webpack.umd.config.babel.js",
    "prebuild": "rimraf build && mkdirp build",
    "test": "eslint src",
    "release": "node ./script/release.js",
    "format": "prettier --write 'src/**/*.js'",
    "format-preview": "prettier --list-different 'src/**/*.js'",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "main": "./build/netlify-gocommerce-widget",
  "keywords": [
    "netlify",
    "gocommerce",
    "ecommerce"
  ],
  "license": "MIT",
  "author": "Matt Biilmann <matt@netlify.com>",
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "babel-cli": "^6.5.2",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "caniuse-lite": "^1.0.30000787",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.0.0",
    "file-loader": "^1.1.6",
    "gotrue-js": "^0.9.15",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "mobx": "^3.2.2",
    "mobx-preact": "^1.1.0",
    "npm-run-all": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.3",
    "postcss-nested": "^3.0.0",
    "preact": "^8.1.0",
    "preact-compat": "^3.17.0",
    "prettier": "^1.6.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "serve": "^6.0.0",
    "source-map-loader": "^0.2.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.4",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "gocommerce-js": "^4.1.4"
  }
}
